digraph {
	size="8,8"
	Outlook [label=Outlook]
	sunny [label=sunny]
	Outlook -> sunny
	Humidity [label=Humidity]
	sunny -> Humidity
	"high -> no" [label="high -> no" fillcolor=lightgray shape=box style=filled]
	Humidity -> "high -> no"
	"normal -> yes" [label="normal -> yes" fillcolor=lightgray shape=box style=filled]
	Humidity -> "normal -> yes"
	"overcast -> yes" [label="overcast -> yes" fillcolor=lightgray shape=box style=filled]
	Outlook -> "overcast -> yes"
	rain [label=rain]
	Outlook -> rain
	Wind [label=Wind]
	rain -> Wind
	"weak -> yes" [label="weak -> yes" fillcolor=lightgray shape=box style=filled]
	Wind -> "weak -> yes"
	"strong -> no" [label="strong -> no" fillcolor=lightgray shape=box style=filled]
	Wind -> "strong -> no"
}
